- var posts = [];
- for (var k in public.posts._data) {var p = public.posts._data[k];p.key=k;posts.push(p)}
- posts.sort(function(a,b){return b.id-a.id});
h1(style="font-size:2.25em;")
  | ind
  span(style="color:red;font-size: 0.75em;") ☺︎
  span(style="color:green;font-size: 0.75em;") ☺︎
  span(style="color:blue;font-size: 0.75em;") ☺︎
  | rsman
ul
  each post in posts
    li
      small.post-date= post.date
      a(href='/posts/#{post.key}')= post.title
